.flter-image image {
  height: 15rpx;
  width: 25rpx;
  position: absolute;
  right: -40rpx;
  bottom: -5px;
}

.dialog-login {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: rgba(0, 0, 0, 0.5);
  display: flex;
  justify-content: center;
  align-items: center;
  overflow: scroll;
}

.dialog-login .dialog-body {
  width: 100%;
  height: 100%;
  overflow:scroll;
  background: #fff;
  border-radius: 5px;
  padding: 45px 0;
}

.dialog-login .dialog-body .title {
  width: 100%;
  text-align: center;
  line-height: 60rpx;
}

.dialog-login .dialog-body .content {
  width: 100%;
}

.dialog-login .dialog-body button {
  margin-top: 30rpx;
}




.my-title{
  background: rgb(250, 247, 247);
  padding: 20rpx;
}
.title-list{
  width:29%;
  display:block;
  text-align:center;
  margin:3% 1% 3% 1%;
  border:1px solid #ddd;
  background:rgb(250, 247, 247);
  border-radius:6rpx;
  padding:6rpx;
  float:left;
  color: #545252;
}
.btn-submit{
  width:100%;
  background:#4da1ff;
  position:fixed;
  bottom:0px;
  color:#fff;
  border-radius:0px;
}
.active{
  background: #4da1ff;
  color: #fff;
}