}
.QRcode{
  width:100%;
  height:292rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}
.QRcode image{
  width:292rpx;
  height:292rpx;
}
.box-bottom{
  width:100%;
  height:60rpx;
  line-height:60rpx;
}
.box-bottom text{
  width:100%;
