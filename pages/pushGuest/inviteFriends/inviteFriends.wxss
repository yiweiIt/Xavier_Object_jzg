page{background:rgb(242,242,242)}
.banner{
  width:100%;
  height:216rpx;
  background: red;
}
.banner button{
  width:100%;
  height:100%;
  margin:0rpx ;
  padding:0rpx;
  border:none;
  border-radius:0;
}
.banner image{
  width:753rpx;
  height:216rpx;
}
.noData{
  width:100%;
  height:400rpx;
  margin-top:100rpx;
}
.imgBox{
   width:100%;
   height:320rpx;
   display: flex;
   align-items: center;
   justify-content: center;
} 
.imgBoxSmall{
   width:100%;
   height:200rpx;
}
.imgBoxSmall  image{
  width:254rpx;
  height:200rpx;
}
.noData image{
   width:407rpx;
   height:320rpx;
}
.noData text.tip,.sreachNo text.tip{
   display: block;
   width:100%;
   height:80rpx;
   line-height: 80rpx;
   text-align:center;
   color:#333;
   font-size:32rpx;
}
.sreachNo button{
   width:40%;
   height:70rpx;
   line-height:70rpx;
   background:#4da1ff;
   border-radius:30rpx;
   margin:0rpx auto;
   border:none;
  color:#fff;
  font-size:30rpx;
}
.invite{
  width:100%;
}
.invitePeople{
  width:100%;
  height:230rpx;
  background:#fff;
  display: flex;
  margin-bottom:30rpx;
}
.peopleAvtar{
  flex:0 0 24%;
  height:100%;
  display: flex;
  align-items: center;
  justify-content: flex-end;
}
.peopleAvtar image{
  width:160rpx;
  height:160rpx;
  border-radius:50%;
  background:rgb(242,242,242);
}
.peopleDetails{
  width:74%;
  height:100%;
  display: flex;
  align-items: center;
}
.detailsBox{
  width:100%;
  height:165rpx;
}
.detilsList{
   width:100%;
   height:55rpx;
   line-height:55rpx;
   display: flex;
   align-items: center;
   justify-content: flex-start; 
}
.detailIcon{
  flex:0 0 12%;
  height:100%;
  display: flex;
  align-items: center;
  justify-content: center;
}
.detilsList image{
  width:36rpx;
  height:36rpx;
}
.detilsList text{
   color:#666;
   font-size:30rpx;
}
.detilsList text.detailName{
   display: block;
   flex:0 0 22%;
   height:100%;
}
.detilsList text.detailsName{
   flex:0 0 26%;
   height:100%;
   font-size:30rpx;
}
.detilsList text.detailsInfo{
   display: block;
   flex:1;
   height:100%;
}
.overflow{
   overflow:hidden;
   text-overflow:ellipsis;
   white-space:nowrap;
}
.inviteList{
  width:100%;
  min-height:calc(100vh - 200rpx);
  background:#fff;
}
.listTitle{
  width:92%;
  height:110rpx;
  margin:0rpx auto;
  display: flex;
  align-items: center;
  justify-content: flex-start;
}
.listTitle text{
   height:40rpx;
   line-height: 40rpx;
   padding:0rpx 5rpx 0rpx 15rpx;
   font-size:30rpx;
   color:#333;
}
.listTitle text:nth-child(1){
  border-left:4rpx solid #4da1ff;
}
.listTitle text:nth-child(2){
  font-size:26rpx;
}
.listSearch{
  width:92%;
  margin:10rpx auto 20rpx auto;
  height:80rpx;
  box-shadow:0rpx 1rpx 10rpx rgba(47, 52, 58, 0.08);
}
.search{
  width:84%;
  padding:0rpx 2%;
  background:#fff;
  border-radius:6rpx;
  font-size:30rpx;
  color:#333;
  height:100%;
  float:Left;
  border-top-right-radius:0rpx;
  border-bottom-right-radius:0rpx;
}
.searchBg{
   width:74%;
}
.imageBoxD{
   width:10%;
   height:100%;
   float:Left;
   background:#fff;
}
.imageBoxD image{
  width:32rpx;
  height:32rpx;
}
.imageBox{
   width:12%;
   background:#4da1ff;
   height:100%;
   float:right;
   display: flex;
   align-items: center;
   justify-content: center;
   border-top-right-radius:10rpx;
   border-bottom-right-radius:10rpx;
}
.imageBox image{
  width:32rpx;
  height:35rpx;
}
.sreachNo{
  width:92%;
  height:360rpx;
  margin:40rpx auto;
}
.listBox{
  width:92%;
  height:160rpx;
  background:#fff;
  margin:30rpx auto;
  border-radius:12rpx;
  box-shadow:0rpx 1rpx 10rpx rgba(47, 52, 58, 0.08);
  display: flex;
  align-items: center;
}
.listImg{
  width: 16%;
  height:100%;
  justify-content: flex-start!important;
  margin-left:3%;
}
.listImg image{
   width:94rpx;
   height:94rpx;
   border-radius:50rpx;
   background: #efefef;
}
.listContent{
  width:61%;
  height:120rpx;
  color:#222;
  font-size:28rpx;
}
.listContent text.name,.listContent text.time{
  display: block;
  width: 100%;
  height:40rpx;
  line-height: 40rpx;
  font-size:28rpx;
}
.listContent text.time{
  color:#333;
  font-size:24rpx;
}
.operate{
  width:20%;
  height:100%;
}
.operate button{
  width:100rpx;
  height:54rpx;
  line-height:54rpx;
  background:#4da1ff;
  border:none;
  font-size:28rpx;
  color:#fff;
  padding:0rpx;
  margin:0rpx;
}
.flex{
  display: flex;
  align-items: center;
  justify-content: center;
}
.dpn{
  display: none;
}
