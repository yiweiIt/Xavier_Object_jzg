page{background:rgb(242,242,242)}
.noData{
  position:absolute;
  width:100%;
  height:700rpx;
  display:-webkit-flex;
  display:flex;
  align-items: center;
  justify-content: center;  
  align-content:center; 
  flex-wrap:wrap;
}
.dataImg{
  width:100%;
  height:400rpx;
  display: flex;
  align-items: flex-end;
  justify-content: center;
}
.dataImg image{
   width:407rpx;
   height:320rpx;
}
.dataText{
  width:100%;
  height:60rpx;
  line-height:60rpx;
  color:#333;
  font-size:32rpx;
  text-align:center;
}
.notesList{
  width:100%;
  height:160rpx;
  background:#fff;
  border-bottom:1rpx solid #f2f2f2;
}
.notesList:last-child{
  border:none;
}
.flex{
  display:-webkit-flex;
  display: flex;
  align-items: center;
  justify-content: center;
}
.flexStart{
  display:-webkit-flex;
  display: flex;
  align-items: center;
  justify-content: flex-start;
}
.listBox{
   width:92%;
   height:120rpx;
}
.photoBox{
  width:130rpx;
  height:100%;
}
.photoBox image{
  width:100rpx;
  height:100rpx;
}
.contentBox{
  flex:1;
  height:100rpx;
}
.contentBox text{
   display: block;
   width:100%;
   height:50rpx;
   line-height:50rpx;
}
.contentBox text.title{
  color:#333;
  font-size:30rpx;
}
.contentBox text.time{
  color:#666;
  font-size:28rpx;
}
.consumption{
  width:100rpx;
  height:120rpx;
  line-height:120rpx;
  color:#f48642;
  font-size:32rpx;
  text-align:center;
}