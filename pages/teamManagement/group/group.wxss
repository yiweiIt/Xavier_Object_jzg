page{background-color:rgba(242, 242, 242, 1);}
.group{
  padding: 0rpx;
  margin:0rpx;
}
.leader{
  width:100%;
  height:auto;
}
.dataListBox{
  overflow: hidden;
  /* height: calc(100vh - 156rpx); */
  height: calc(100vh);
  /* padding-bottom:130rpx; */
}
._dataListBox{
    overflow: hidden;
    height: calc(100vh);
}

.prop{
  position: fixed;
  width: 100%;
  height: 100%;
  background-color: rgba(0,0,0,0.7);
  top: 0;
  left: 0;
  display:-webkit-flex;
  display:flex;
  align-items: center;
  justify-content: center;
  z-index: 998;
  align-content:center; 
  flex-wrap:wrap;
}

.add_model{
  width: 84%;
  background-color: #ffffff;
  border-radius: 20rpx;
  z-index: 999;

}
.add_model{
  padding: 20rpx 0rpx;
}

.add_model button{
  height: 65rpx;
  line-height: 65rpx;
  width: 40%;
  margin-top: 60rpx;
  font-size: 34rpx;
  border-radius: 4rpx;
  color: #666666;
}
.add_model button button::after {
  border: none;
}

.add_model text{
  height:80rpx;
  line-height:80rpx;
  font-size:36rpx;
  display: block;
  text-align: center;
  color:#222;
}
.dpn{
  display:none
}
.scanCard{
  width:16%;
  height:70rpx;
  float:right;
  display: flex;
  align-items: center;
  justify-content: center;
}
.scanCard image{
  display:block;
  width:50rpx;
  height:50rpx;
}
.propTiper{
   width:100%;
   height:40rpx;
   line-height: 40rpx;
   font-size:26rpx;
   text-align: center;
   color:#333;
}
.propRed{
    color:#FF0000;
    padding:0rpx 5rpx;
}
.addInputBox{
   width:94%;
   height:630rpx;
   margin:0rpx auto;
}
.addInputList{
  width:100%;
  height:80rpx;
  border-bottom:1rpx solid #efefef;
  margin:1rpx auto;
}
.imageBox{
  width:16%;
  height:100%;
  float:left;
  display: flex;
  align-items: center;
  justify-content: center;
}
.imageBox image{
    width:46rpx;
    height:46rpx;
}
.imageBox image.bank{
    width:42rpx;
    height:42rpx;
}
.imageBox image.card{
    width:42rpx;
    height:32rpx;
}
.imageBox image.camera{
    width:46rpx;
    height:37rpx;
}
.symbol{
  width:4%;
  height:100%;
  /* background:green; */
  float:left;
  color:#ff0000;
  display: flex;
  align-items: center;
}
.add_input,.add_input_left,.listRight{
  width:80%;
  height:80rpx;
  line-height: 80rpx;
  border:none;
  float:left;
  color: #666;
  font-size:30rpx;
}
.listRight picker .flter-itm1 input,.listRight picker input{
  width:100%;
  height:80rpx;
  line-height: 80rpx;
}
.add_input_left{
  width:62%;
}
.add_model button.addSubmit{
  width:70%;
  height:70rpx;
  line-height:70rpx;
  background:rgb(77,161,255);
  color:#fff;
  border-radius:40rpx;
  font-size: 30rpx;
  margin-top:40rpx;
}
.cancelBox{
   width:100%;
   height:140rpx;
   display: flex;
   align-items: center;
   justify-content: center;
}
.cancelBox image{
  width:60rpx;
  height:60rpx;
}
.footer{
  bottom:40rpx;
  left:4%;
  position:fixed;
  width:92%;
  height:90rpx;
  z-index:99;
}
.footer button{
  width:50%;
  height:90rpx;
  line-height:90rpx;
  border:none;
  background:none;
  outline: none; 
  text-align:center; 
  color:#fff;
  font-size:32rpx;
  border-radius:0rpx;
  float:left;
}
.footer button:first-child{
  background:rgb(77,161,255);
  border-top-left-radius: 45rpx;
  border-bottom-left-radius: 45rpx;
}
.footer button:last-child{
  background:rgb(40,131,232);
  border-top-right-radius: 45rpx;
  border-bottom-right-radius: 45rpx;
}
.footer button::after{
  border:none;
}
.teamLeaderBox{
  width:100%;
  height: 240rpx;
  background: #fff;
  margin:0rpx auto 25rpx auto;
  display: flex;
  align-items: center;
  justify-content: center;
}
.leaderLeftBox{
  width:92%;
  height:180rpx;
}
.leaderLeft{
  width:30%;
  height: 100%;
  float:left;
  display: flex;
  align-items: center;
  justify-content: center;
}
.leaderLeft image{
   width:160rpx;
   height:160rpx;
   border-radius:10rpx;
}
.leaderRight{
  width:68%;
  height: 100%;
  float:right;
  margin-right:2%;
}
.leaderBox{
  width:100%;
  height:60rpx;
  line-height:60rpx;
}
.leaderImage{
  width:15%;
  height:100%;
  float:left;
  display: flex;
  align-items: center;
  justify-content: center;
}
.leaderImage image{
  width:34rpx;
  height:34rpx;
}
text.leaderTopic{
  display: inline-block;
  width:28%;
  height:100%;
  float:left;
  font-size:30rpx;
  color:#222;
}
text.small{
  font-size:28rpx;
  color:#666;
}
.leaderRightBox{
  width:57%;
  height:100%;
  float:right;
}
.leaderRightBox text.leaderName{
  display: inline-block;
  min-width:30%;
  max-width: 40%;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  float:left;
  font-size:30rpx;
  color:#222;
}
.leaderTopicR{
  display: inline-block;
  width:57%;
  height:60rpx;
  line-height:60rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.starBox{
  min-width:60%;
  max-width:70%;
  height:100%;
  float:left;
}
.starBox image.Orange,.groupStarBox image.Orange{
   width:30rpx;
   height:30rpx;
}
.starBox image.dark,.groupStarBox image.dark{
   width:28rpx;
   height:28rpx;
}
.groupStarBox image.Orange,.groupStarBox image.dark{
  padding:0rpx 4rpx;
}
.memberList{
  width:100%;
  height:110rpx;
  line-height:110rpx;
  background:#fff;
  margin:10rpx auto;
}
.memberList:last-child{
  margin-bottom:150rpx;
}
.groupListBox{
  width:96%;
  height:100%;
  margin:0rpx auto;
}
.groupImage{
  width:16%;
  height:110rpx;
  float:left;
  display: flex;
  align-items: center;
  justify-content: center;
}
.groupImage image{
  width:80rpx;
  height:80rpx;
  border-radius:50%;
}
.groupCenter{
   width:54%;
   height:100%;
   float:left;
}
text.classification{
  display: inline-block;
  width:24%;
  height:100%;
  float:left;
  text-align:right;
  color:rgb(153,153,153);
  font-size:27rpx;
}
.nextBox{
  width:6%;
  height:100%;
  float:right;
  display: flex;
  align-items: center;
  justify-content: center;
}
.nextBox image{
  width:10rpx;
  height:24rpx;
}
.groupCenterName{
  display: block;
  width:100%;
  margin-top:5rpx;
  height:50rpx;
  line-height:60rpx;
  color:rgb(34,34,34);
  font-size:30rpx;
  text-indent:4rpx; 
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.groupStarBox{
  width: 100%;
  height:50rpx;
  line-height:50rpx;
}