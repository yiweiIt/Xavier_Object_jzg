/* pages/teamManagement/contract/contract.wxss */
page{background:rgba(242,242,242,1)} 
.contractList{
  width:92%;
  height:500rpx;
  background:#fff;
  margin:24rpx auto;
  border-radius: 10rpx;
}
.companyName{
  display:block;
  width:100%;
  height:100rpx;
  line-height:100rpx;
  text-align:center;
  color:#333;
  font-size:34rpx;
  border-bottom:2rpx solid rgb(242, 242, 242,1);
}
.contractInfo{
  width:92%;
  height:260rpx;
  margin:20rpx auto;
}
.contractInfo text{
  display:block;
  width:100%;
  height:65rpx;
  line-height:65rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: normal;
  font-size:30rpx;
  color:#aaa;
}
.contractBtn{
  width:100%;
  height:100rpx;
  line-height:100rpx;
  text-align:center;
  color:#4b9f7d;
  font-weight:bold;
  font-size:32rpx;
  border-top:2rpx dashed rgb(201, 201, 201,1);
}
.boxBg{
  width:100%;
  height:100%;
  position:absolute;
  display: flex;
  align-items: center;
  justify-content:center;
}
.boxBg image{
  width:160rpx;
  height:160rpx;
  display:block;
  margin:0rpx auto;
}
.boxBg text{
  width:100%;
  display: block;
  line-height:50rpx;
  text-align: center;
  color:#888;
}