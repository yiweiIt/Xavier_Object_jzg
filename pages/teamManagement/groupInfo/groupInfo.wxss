page{background: #f2f2f2;}
.infomation{width:100%;}
.upper_apex{
  width:100%;
  height:200rpx;
  background:  url(http://static.jianzhugang.com/mini/image/upper_apex_bg.png) no-repeat 50% 50%;
  background-size: 100% 100%;
}
.projectInfo{
  height:760rpx;
  /* margin-top:-100rpx; */
  margin:-100rpx auto 30rpx auto;
  position:relative;
}
.imageBox{
  width:100%;
  height:160rpx;
  /* background:rgba(194, 247, 2, 0.4); */
  position:absolute;
  margin-top:-80rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}
.image_box{
  width:160rpx;
  height:160rpx;
  border-radius:50%;
  background:#4da1ff;
  display: flex;
  align-items: center;
  justify-content: center;
}
.imageBox image{
  width:144rpx;
  height:144rpx;
  border-radius:50%;
}
.infoListBox{
  width:92%;
  padding-top:100rpx;
  margin:0rpx auto;
}
.infoList{
  width:100%;
  height:90rpx;
  line-height:90rpx;
  border-bottom:1rpx solid #f2f2f2;
  font-size:30rpx;
  color:#222;
}
.lastInfo{
  border:none;
}
.listName{
  display: inline-block;
  width:30%;
  height:100%;
  float:left;
}
.inputBox{
  width:70%;
  height:100%;
  float:left;
  text-align:right;
}
.small{
  width:66%;
}
.big{
  width:56%;
}
.iconNext{
  width:4%;
  height:100%;
  float:right;
  display: flex;
  align-items:center;
  justify-content: center;
}
.iconNext image{
  width:9rpx;
  height:22rpx;
}
.cardBox,.scanCard{
  width:14%;
  height:90rpx;
  float:right;
  display: flex;
  align-items:center;
  justify-content: center;
}
.cardBox image{
  display: block;
    width:50rpx;
    height:40rpx;
}
.inputBox input{
  width:100%;
  height:90rpx;
  line-height:90rpx;
}
.box{
  width:92%;
  background:#fff;
  border-radius:16rpx;
}
.boxList{
  margin:0rpx auto;
  /* height:360rpx; */
  height:auto;
}
.starBox{
  width:92%;
  height:90rpx;
  line-height:90rpx;
  border-bottom:1rpx solid #f2f2f2;
  margin:0rpx auto;
  padding-top:10rpx;
}
.iconBox{
  width:54rpx;
  height:100%;
  float:Left;
  display: flex;
  align-items: center;
  justify-content: flex-start;
}
.iconBox image{
  width:40rpx;
  height:40rpx;
}
.starName,.starRemarks{
  display: inline-block;
  width:30%;
  height:100%;
  float:Left;
  font-size:30rpx;
  color:#222;
}
.starRemarks{
  width:88%;
}
.starList{
  width:58%;
  height:100%;
  float:right;
  display: flex;
  align-items:center;
  justify-content: flex-end;
}
.starList image.Orange{
    width: 40rpx;
    height: 40rpx;
}
.starList image.dark{
    width: 38rpx;
    height: 38rpx;
}
.remarks{
    width:92%;
    height:80rpx;
    line-height:80rpx;
    margin:0rpx auto; 
}
.details{
   width:92%;
   margin:0rpx auto;
   min-height:100rxp;
   font-size:30rpx;
   color:#222;
   padding-bottom:40rpx;
}
.details input{
  width:100%;
  height:100rpx;
  line-height: 100rpx;
  border-bottom:1rpx solid #f2f2f2;
}
.detailsBox{
  display: block;
  line-height:50rpx;
}
.operate{
  width:82%;
  height:120rpx;
  margin:50rpx auto;
}
.buttonCenter{
  width:44%;
  height:70rpx;
  border:2rpx solid rgb(77,161,255);
  float:left;
  border-radius:8rpx;
}
.buttonCenter text,.buttonCenter button{
   display: block;
   width:100%;
   height:70rpx;
   line-height: 70rpx;
   text-align: center;
   margin:0rpx auto;
   border-radius:6rpx;
   color:#fff;
   font-size:30rpx;
}
.buttonCenter:nth-child(1){
  float:left;
}
.buttonCenter:nth-child(2){
  float:right;
}
.buttonCenter:nth-child(1) button{
  background:rgb(77,161,255);
}
.buttonCenter:nth-child(2) text{
  color:rgb(77,161,255);
}
.dpn{
  display: none;
}