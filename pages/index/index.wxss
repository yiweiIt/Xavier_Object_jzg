/* 灰色背景 */
page{
  background-color: rgb(242,242,242);
}
view{
  margin: 0px;
  padding: 0px;
}
button{
  padding-left:0px;
  padding-right:0px;
  font-size:0px;
  line-height: 1.55555556;
  display:inline
}
button::after{
  border:none;
}

/*轮播图  */
 .slide-image{
  width: 750rpx;
  height: 300rpx;
}
/*logo  */
._logoBoxBg{
    width: 100%;
    height: 350rpx;
    background-color:#fff;
    display: flex;
    align-items: center;
    justify-content: center;
}
.logoBox{
    width:92%;
    height:320rpx;
    display: flex;
    flex-wrap: wrap;
    align-content: flex-start;
}
.logoList{
  flex: 0 0 25%;
  height:160rpx;
}
.listImage{
   width:100%;
   height:100rpx;
   display: flex;
   align-items: center;
   justify-content: center;
}
.listImage image{
   width:86rpx;
   height:86rpx;
}
.listTip{
  display: block;
  width:100%;
  height:50rpx;
  line-height: 50rpx;
  font-size:30rpx;
  color:#333;
  text-align:center;
}
/*公告  */
.affiche{
  position: relative;
  overflow: hidden;
  float: left;
  width: 100%;
  height:100rpx;
  background-color: white;
  margin:20rpx 0rpx;
  display: flex;
  align-items: center;
  justify-content: center
}
.announcement{
   width:92%;
   height: 100rpx;
   line-height:100rpx;
   display: flex;
   align-items: center;
}
.notice{
   flex:0 0 9%;
   height:100%;
   display: flex;
   align-items: center;
   justify-content: flex-start;
}
.notice image{
   width:45rpx;
   height:36rpx;
}
.affiche_text{
  flex:1;
  height:60rpx;
  text-align: left;
}
.tab-right,.right-item{
  width:100%;
  height:100%;
}
.content-item {
    height: 60rpx;
    line-height:60rpx;
    display: flex;
    align-items: center;
}
.name {
    display: block;
    font-size: 26rpx;
    height:100%;
    color: #333;
    overflow:hidden;
    text-overflow:ellipsis;
    white-space:nowrap;
}
/*急聘专区*/
.new{
  float: left;
  background-color: #fff;
  width: 100%;
  height:100rpx;
  font-size: 28rpx;
}
.hiredBox{
  width:92%;
  height:100rpx;
  line-height:100rpx;
  margin:0rpx auto;
  display: flex;
}
.hiredTitle{
  flex:0 0 70%;
  height:100%;
  display: flex;
  align-items: center;
  justify-content: flex-start;
}
.contnetTitle{
  display: block;
  width:100%;
  height:40rpx;
  line-height:40rpx;
  border-left:6rpx solid #4da1ff;
  text-indent:16rpx;
  color:#333;
  font-size:34rpx;
}
.hiredmore{
  flex:0 0 26%;
  height:100%;
  display: flex;
  align-items: center;
  justify-content: flex-end;
}
.hiredmore text.more{
  color:#666;
  font-size:28rpx;
}
.hiredIcon{
  flex:1;
  display: flex;
  align-items: center;
  justify-content: flex-end;
}
.hiredIcon image{
  width:12rpx;
  height:29rpx;
}
/*最新招工列表  */
.urgentlyHiredBox{
  float: left;
  width:100%;
  background-color: white;
  overflow:hidden;
}
.listBox{
  width:92%;
  height:355rpx;
  background:#fff;
  border-radius:12rpx;
  margin:26rpx auto;
  box-shadow: 0rpx 2rpx 12rpx rgba(74, 74, 74, 0.15); 
  position:relative;
}
.contentBox{
   width:88%;
   height:310rpx;
   position:absolute;
   top:19rpx;
   left:6%;
   z-index:3;
}
.statusBox{
   z-index:9;
   width:22%;
   height:70rpx;
   position:absolute;
   top:0rpx;
   right:1%;
   display: flex;
   justify-content: center;
}
.statusBox image{
   width:103rpx;
   height:66rpx;
}
.hiderTip{
   z-index:11;
   width:76rpx;
   height:78rpx;
   position:absolute;
   top:0rpx;
   left:0rpx;
}
.hiderTip image{
  width:100%;
  height:100%;
}
.title{
  display:block;
  width:80%;
  height:68rpx;
  line-height:68rpx;
  color:#333;
  font-size:30rpx;
}
.overhide{
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.info{
  width:100%;
  height:60rpx;
  line-height:60rpx;
  display: flex;
}
.icon{
  flex:0 0 8%;
  height:100%;
  display: flex;
  align-items: center;
}
.icon image{
  width:34rpx;
  height:34rpx;
}
.icon image.imageIcon{
  width:32rpx;
  height:32rpx;
}
.infoText{
  flex:0 0 22%;
  height:100%;
  color:#666;
  font-size:28rpx;
}
.infomation{
  flex:auto;
  height:100%;
  color:#666;
  font-size:28rpx;
  display: flex;
  align-items: center;
  justify-content: flex-start;
}
.infomation image{
   width:30rpx;
   height:28rpx;
   padding:0rpx 8rpx 0rpx 0rpx;
}
.getMore{
  width:100%;
  height:80rpx;
  line-height: 80rpx;
  text-align: center;
  font-size:30rpx;
  color: rgb(77,161,255);
}
.swpier-it navigator{
  height: 100%;
  width: 100%;
}
.swpier-it navigator image{
  height: 100%;
  width: 100%;
}
.more-btn{
  cursor: pointer;
  padding: 30rpx 0;
  text-align: center; 
  font-size: 30rpx;
  color:  rgba(77,161,255,1);
}
.content-listlide-image{
  height: 100%;
  width: 100%;
  background-size: 100% 100%;
  background-repeat: no-repeat;
  background-position: center;
}
.hide{
  display: none;
}
.swiper{
  height: 150px;
}
.singin{
  /* background:#0099FF; */
  color:#fff;
  position:fixed;
  width:200rpx;
  height:151rpx;
  top:75%;
  right:0rpx;
  text-align:center;
}
.singin image{
  width:200rpx;
  height:151rpx;
}
.situation{
  background:#0099FF;
  /* background:#0099FF; */
  color:#fff;
  position:fixed;
  width:110rpx;
  height:110rpx;
  top:56%;
  right:20rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}
.situation image{
  width:70rpx;
  height:70rpx;
  
}
.publish{
  background:#0099FF;
  color:#fff;
  position:fixed;
  width:40%;
  padding:5px;
  bottom:10px;
  left:30%;
  font-size:36rpx;
  text-align:center;
  border-radius:50px;
  box-shadow:2px 0px 6px rgb(0, 153, 255);
  letter-spacing:5px;
  z-index:99;
}
.dpn{
  display: none;
}
.singiner{
  background:#0099FF;
  /* background:rgb(0, 153, 255,0.8); */
  color:#fff;
  position:fixed;
  width:130rpx;
  height: 68rpx;
  line-height: 68rpx;
  top:75%;
  right:0;
  font-size:26rpx;
  text-align:center;
  border-top-left-radius: 30rpx;
  border-bottom-left-radius: 30rpx;
}
.pushGuest{
  position:fixed;
  width:160rpx;
  height:154rpx;
  bottom:7%;
  right:0rpx;
  z-index: 999999;
}
.pushGuest image{
  width:160rpx;
  height:154rpx;
}
.prop{
  position: fixed;
  width: 100%;
  height: 100%;
  background-color: rgba(0,0,0,0.7);
  top: 0;
  left: 0;
  display:-webkit-flex;
  display:flex;
  align-items: center;
  justify-content: center;
  z-index: 998;
  align-content:center; 
  flex-wrap:wrap;
}

.add_model,.pushOffBox{
  width: 70%;
  background-color: #ffffff;
  border-radius: 20rpx;
  z-index: 999;
  height:520rpx;
}
.pushOffBox{
  background:rgba(242,242,242,0);
  position:relative;
}
.modelTitle{
   width:100%;
   height:120rpx;
   background:url(https://jianzhugang.oss-cn-shenzhen.aliyuncs.com/mini/newIcon/indexTeamBg.png) 0% 0% no-repeat;
   background-size:100%;
   position:relative;
   display: flex;
   justify-content: center;
}
.modelTitle image{
  position:absolute;
  width:188rpx;
  height:120rpx;
  top:-68rpx;
}
.titleText{
  width:84%;
  height:90rpx;
  line-height:90rpx;
  margin-left:8%;
  display: flex;
}
.boderBox{
  flex:0 0 30%;
  height:100%;
  display: flex;
  align-items: center;
}
.boderBox image{
  width:120rpx;
  height:10rpx;
}
.textTip{
  flex:1;
  height:100%;
  text-align:center;
  font-size:34rpx;
  color:#333;
}

.add_model button{
  height: 65rpx;
  line-height: 65rpx;
  width: 40%;
  margin-top: 60rpx;
  font-size: 34rpx;
  border-radius: 4rpx;
  color: #666666;
}
.add_model button button::after {
  border: none;
}

.add_model text{
  height:80rpx;
  line-height:80rpx;
  font-size:36rpx;
  display: block;
  text-align: center;
  color:#222;
}
.dpn{
  display:none
}
.pushOffBox_cancel{
   position:absolute;
   top:0rpx;
   right:0rpx;
   height:80rpx;
   width:80rpx;
   display: flex;
   align-items: center;
   justify-content: center;
}
.pushOffBox_cancel image{
  width:52rpx;
  height:52rpx;
}
.pushOffBoxBg{
  width:100%;
  height:100%;
  display: flex;
  align-items: center;
  justify-content: center;
}
.pushOffBoxBg image{
  width:500rpx;
  height:522rpx;
}
.cancelBox,.participate{
   width:100%;
   height:140rpx;
   display: flex;
   align-items: center;
   justify-content: center;
}
.cancelBox image{
  width:60rpx;
  height:60rpx;
}
.participateImg{
  width:349rpx;
  height:66rpx;
}
.modeContent{
  width:84%;
  line-height: 50rpx;
  margin:0rpx auto;
  color:#666;
  font-size:30rpx;
  padding:10rpx 0rpx ;
}
.transform{
  transform:rotateY(180deg);
  -webkit-transform: rotateY(180deg);
  -moz-transform: rotateY(180deg);
  -o-transform: rotateY(180deg);
  -ms-transform: rotateY(180deg);
}
.examine{
   width:40%;
   height:60rpx;
   line-height:60rpx;
   background:#4da1ff;
   border-radius:30px;
   box-shadow:2px 0px 6px rgb(0, 153, 255);
   color:#fff;
   margin:20rpx auto;
   text-align: center;
   font-size:32rpx;
}
