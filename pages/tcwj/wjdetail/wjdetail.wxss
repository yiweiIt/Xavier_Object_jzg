page{
  background:rgba(242,242,242,1);
}
.detailsBox{
  width:100%;
  height:200rpx;
  background:#4da1ff;
  display: flex;
  justify-content: center;
  font-size:28rpx;
}
.detailsBox image{
   width:150rpx;
   height:150rpx;
   border-radius:50%;
   margin-top:10rpx;
   z-index:3;
   border:4rpx solid #4da1ff;
}
.basis{
  z-index:2;
  width:92%;
  /* height:200rpx; */
  background:#fff;
  border-radius:8rpx;
  padding:16rpx 0rpx;
  font-size:28rpx;
}
.basistip{
  margin:-100rpx auto 10rpx auto;
}
.center{
  margin:20rpx auto;
}
text.basisText{
   width:92%;
   margin:0rpx auto;
   height:80rpx;
   line-height: 80rpx;
   display: block;
   color:#666;
   border-bottom: 2rpx solid rgba(242,242,242,1);
}
.listBox{
  width:92%;
  margin:0rpx auto;
  height:90rpx;
  line-height: 90rpx;
  color:#666;
  border-bottom:2rpx solid rgba(242,242,242,1)
}
.listBox text{
   width:30%;
   float:left;
   height:100%;
   color:#333;
}
.listBox input,.listBox picker{
   width:70%;
   float:right;
   height:90rpx;
   line-height: 90rpx;
   text-align:right;
}
.listBox:last-child{
  border:none;
}
.reportContent{
  width:92%;
  margin:0rpx auto;
  height:280rpx;
  background:#fff;
  padding:15rpx 0rpx;
  border-radius: 6rpx;
}
.reportContent text{
  border:none;   
  height:60rpx;
  line-height: 60rpx;
}
.input-area{
  width:90%;
  height:180rpx;
  background:rgba(242,242,242,1);
  border:none;
  margin:0rpx auto;
  border-radius:4rpx;
  color:#666;
}
.basisText text.circle {
  display: inline-block;
   width: 16rpx;
   height: 16rpx;
   background: #4da1ff;
   border-radius: 50%;
   margin: 0rpx 12rpx 0rpx 0rpx;
 }
 .star{
   float:right;
   width:70%;
   float:right;
   height:90rpx;
   line-height: 90rpx;
 }
 .star image{
    width:40rpx;
    height:40rpx;
    display:inline-block;
    float:right;
    margin-top:25rpx;
 }
  