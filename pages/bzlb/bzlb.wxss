page{
  background:rgba(242,242,242,1);
  color:#333;
}
.area{
  position: fixed;
  width: 100%;
  height: 120rpx;
  background:#fff;
  display: flex;
  align-items: center;
  justify-content: center;
}
.area picker{
  color:rgb(73, 73, 73);
}
/*地区  */
.area .dq {
  font-size: 28rpx;
  width: 49%;
  display: flex;
  align-items: center;
  justify-content: center;
}

.area .gz {
  font-size: 28rpx;
  width: 49%;
  display: flex;
  align-items: center;
  justify-content: center;
  float:left;
}
/*中间分隔线  */
.area .center {
  color: rgba(0, 0, 0, 0, 0.3);
  height: 50rpx;
  flex: 1;
  text-align: center;
}
